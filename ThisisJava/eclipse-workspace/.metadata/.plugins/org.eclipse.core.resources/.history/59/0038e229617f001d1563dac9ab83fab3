package ch08.sec02;

public class Television implements RemoteControl{
	int volume;
	
	public void turnOn() {
		System.out.println("TV를 켭니다.");
	}
	
	public void turnOff() {
		System.out.println("TV를 끕니다.");
	}
	
	public void setVolume(int volume) {
		System.out.println("TV 볼륨을 " + volume + "으로 설정합니다.");
	}
}
