package ch09.sec02.exam01;

public class A {
	class B {
		String sayHello = "Hello. ClassClass!";
		
		void sayHello() {
			System.out.println(sayHello);
		}
	}
	
	B b = new B();
	
	A() {
		B b = new B();
	}
	
	void method() {
		B b = new B();
	}
	
	void useB() {
		B b = new B();
		b.sayHello();
		b.sayHello = "ClassClass Hello!";
		b.sayHello();
	}
}
