package ch06.Question.Banking;

public class Account {
	private int money;
	private String asn, name;
	
	Account(String asn, String name, int money) {
		this.asn = asn;
		this.name = name;
		this.money = money;
	}
	
	String getAsn() {
		return asn;
	}
	
	String getName() {
		return name;
	}
	
	int getMoney() {
		return money;
	}
}
